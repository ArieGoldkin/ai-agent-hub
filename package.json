{
  "name": "ai-agent-hub",
  "version": "3.0.0",
  "type": "module",
  "description": "Transform Claude into 9 specialized AI agents with one command - Zero dependencies orchestration system",
  "author": "Arie Goldkin",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "bin": {
    "ai-agent-hub": "./dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "prettier": "^3.6.2",
    "typescript": "^5.5.4"
  },
  "files": [
    "dist/",
    "agents/",
    ".env.example",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArieGoldkin/ai-agent-hub.git"
  },
  "bugs": {
    "url": "https://github.com/ArieGoldkin/ai-agent-hub/issues"
  },
  "homepage": "https://github.com/ArieGoldkin/ai-agent-hub#readme",
  "keywords": [
    "ai-agents",
    "claude",
    "claude-code",
    "orchestration",
    "zero-dependency",
    "ai-assistant",
    "agent-personalities",
    "studio-coach",
    "anthropic"
  ],
  "dependencies": {
    "chalk": "^5.6.0"
  }
}

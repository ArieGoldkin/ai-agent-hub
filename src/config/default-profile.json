{
  "profile": {
    "name": "Default AI Agent Hub Profile",
    "version": "1.0.0",
    "description": "Default configuration with core servers enabled and optional servers as feature flags"
  },
  "system": {
    "projectRoot": "${PROJECT_ROOT}",
    "memoryBackend": "sqlite",
    "contextDirectory": ".agent-context",
    "logLevel": "info",
    "enableAuditLog": true
  },
  "servers": {
    "enabled": {
      "filesystem": {
        "enabled": true,
        "config": {
          "allowedDirectories": ["${PROJECT_ROOT}"],
          "maxFileSize": "10MB",
          "allowedExtensions": [".ts", ".js", ".json", ".md", ".txt", ".yml", ".yaml", ".py", ".go", ".rs", ".java", ".c", ".cpp", ".h", ".css", ".html", ".vue", ".jsx", ".tsx"]
        },
        "environmentVariables": {
          "PROJECT_ROOT": "${PROJECT_ROOT}"
        },
        "toolOverrides": {
          "read_file": {
            "enabled": true,
            "security": "sandboxed"
          },
          "write_file": {
            "enabled": true,
            "security": "sandboxed"
          },
          "list_directory": {
            "enabled": true,
            "security": "sandboxed"
          },
          "search_files": {
            "enabled": true,
            "security": "sandboxed"
          }
        }
      },
      "git": {
        "enabled": true,
        "config": {
          "allowAutoCommit": false,
          "signCommits": true,
          "workingDirectory": "${PROJECT_ROOT}"
        },
        "environmentVariables": {
          "PROJECT_ROOT": "${PROJECT_ROOT}"
        },
        "toolOverrides": {
          "git_status": {
            "enabled": true,
            "security": "sandboxed"
          },
          "git_commit": {
            "enabled": true,
            "security": "sandboxed"
          },
          "git_diff": {
            "enabled": true,
            "security": "sandboxed"
          },
          "git_log": {
            "enabled": true,
            "security": "sandboxed"
          },
          "git_branch": {
            "enabled": true,
            "security": "sandboxed"
          }
        }
      },
      "github": {
        "enabled": true,
        "config": {
          "authMode": "${GITHUB_AUTH_MODE}",
          "scopes": ["repo", "issues", "pull_requests"],
          "apiVersion": "2022-11-28"
        },
        "environmentVariables": {
          "GITHUB_TOKEN": "${GITHUB_TOKEN}",
          "GITHUB_AUTH_MODE": "${GITHUB_AUTH_MODE}"
        },
        "toolOverrides": {
          "create_pull_request": {
            "enabled": true,
            "security": "authenticated"
          },
          "list_issues": {
            "enabled": true,
            "security": "authenticated"
          },
          "create_issue": {
            "enabled": true,
            "security": "authenticated"
          },
          "get_repository": {
            "enabled": true,
            "security": "authenticated"
          }
        }
      }
    },
    "disabled": []
  },
  "security": {
    "authMode": "none",
    "requireAuthentication": false,
    "sandboxMode": true,
    "allowedOrigins": ["http://localhost:*", "https://localhost:*"],
    "rateLimiting": {
      "enabled": false,
      "requestsPerMinute": 60
    }
  },
  "features": {
    "enableBrowserAutomation": false,
    "enableDocumentationSearch": false,
    "enableSequentialThinking": false,
    "enableGitHubIntegration": true
  }
}